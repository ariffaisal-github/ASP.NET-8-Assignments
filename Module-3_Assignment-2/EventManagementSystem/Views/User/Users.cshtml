@model List<EventManagementSystem.Models.UserViewModel>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Contact</th>
            <th>User Type</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model) {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.ContactNumber</td>
                <td>@user.UserType</td>
                <td>@user.Address</td> 
            </tr>
        }
    </tbody>
</table>
