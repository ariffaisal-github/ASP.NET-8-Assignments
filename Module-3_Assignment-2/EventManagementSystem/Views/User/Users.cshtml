@model List<UserViewModel>

<table class="table table-bordered table-striped">
    <thead class="table-dark">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Contact</th>
            <th>User Type</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model) {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.ContactNumber</td>
                <td>
                    @switch (user.UserType) {
                        case UserType.Admin:
                            @:Admin User
                            break;
                        case UserType.Standard:
                            @:Standard User
                            break;
                        case UserType.External:
                            @:External User
                            break;
                        case UserType.Guest:
                            @:Guest User
                            break;
                        default:
                            @:Unknown
                            break;
                    }
                </td>
                <td>@user.Address</td> 
            </tr>
        }
    </tbody>
</table>
